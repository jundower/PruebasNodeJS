<div class="row m-2 d-print-none" id ="myTab" >
    <div class="col-6 border">
        <div class="row my-1">
            <div class="col-12">
                <div class="row">
                    <label for="codigo_tipo_anexo" class="col-sm-3 col-form-label col-form-label-sm">Tipo Anexo</label>
                    <select class="col-sm-5 form-control form-control-sm c_mantenimientos" id="codigo_tipo_anexo">
                    </select>
                    <div class="col-4 pr-0">
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="checkbox" id="agrupar_cuentas" checked>
                            <label class="form-check-label col-form-label-sm" for="agrupar_cuentas">Mostrar Agrupaciones por Cuentas</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row my-1">
            <div class="col-12">
                <div class="row">
                    <label for="agrupacion1" class="col-sm-3 col-form-label col-form-label-sm">Filtrar x Cuenta</label>
                    <input type="text" id="agrupacion1_text" class="col-sm-5 form-control form-control-sm c_mantenimientos" placeholder="Presionar F4 o Doble Click" tabla="cuentas" readonly>
                    <div class="col-4">
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="checkbox" id="filtrar_cuentas" checked>
                            <label class="form-check-label col-form-label-sm" for="filtrar_cuentas">Mostrar Todos</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row my-1">
            <div class="col-12">
                <div class="row">
                    <label for="agrupacion2" class="col-sm-3 col-form-label col-form-label-sm">Filtrar x Anexo</label>
                    <input type="text" id="agrupacion2_text" class="col-sm-5 form-control form-control-sm c_mantenimientos" placeholder="Presionar F4 o Doble Click" tabla="anexos" readonly>
                    <div class="col-4">
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="checkbox" id="filtrar_anexos" checked>
                            <label class="form-check-label col-form-label-sm" for="filtrar_anexos">Mostrar Todos</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-6 border">
        <div class="row my-1">
            <div class="col-4">
                <div class="row">
                    <label for="ejercicio" class="col-sm-6 col-form-label col-form-label-sm">Ejercicio</label>
                    <select id="ejercicio" class="col-sm-6 form-control form-control-sm text-center"></select>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <label for="periodo_inicio" class="col-sm-6 col-form-label col-form-label-sm">Fecha Inicio</label>
                    <select id="periodo_inicio" class="periodos col-sm-6 form-control form-control-sm text-center"></select>
                </div>
            </div>
            <div class="col-4">
                <div class="row">
                    <label for="periodo_final" class="col-sm-6 col-form-label col-form-label-sm">Fecha Final</label>
                    <select id="periodo_final" class="periodos col-sm-6 form-control form-control-sm text-center"></select>
                </div>
            </div>
        </div>
        <div class="row my-1">
            <div class="col-6"></div>
            <div class="col-6 text-right">
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="checkbox" id="documentos_pendientes" checked>
                    <label class="form-check-label col-form-label-sm" for="documentos_pendientes">Mostrar solo Documentos Pendientes</label>
                </div>
            </div>
        </div>
        <div class="row my-1">
            <div class="col-4"></div>
            <div class="col-4"></div>
            <div class="col-4">
                <button type="button" class="form-control form-control-sm btn btn-primary btn-block" id ="filtrar">Filtrar</button>
            </div>
        </div>
    </div>
</div>

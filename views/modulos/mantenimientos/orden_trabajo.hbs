<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_codigo" class="col-sm-4 col-form-label col-form-label-sm">Codigo:</label>
                            <input type="text" id="tablas_orden_trabajo_codigo" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Codigo}}" placeholder="Codigo" maxlength="50">
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_abreviatura" class="col-sm-4 col-form-label col-form-label-sm">Abreviatura:</label>
                            <input type="text" id="tablas_orden_trabajo_abreviatura" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Abreviatura}}" placeholder="Abreviatura">
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_nombre" class="col-sm-2 col-form-label col-form-label-sm">Nombre:</label>
                            <input type="text" id="tablas_orden_trabajo_nombre" class="col-sm-10 form-control form-control-sm m-0 p-0"value="{{datos.Nombre}}" placeholder="Nombre" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_orden_servicio" class="col-sm-4 col-form-label col-form-label-sm">Orden Servicio:</label>
                            <input type="text" id="tablas_orden_trabajo_orden_servicio" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Orden_Servicio}}" placeholder="Orden Servicio" >
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_presupuesto" class="col-sm-4 col-form-label col-form-label-sm">Presupuesto:</label>
                            <input type="number" id="tablas_orden_trabajo_presupuesto" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Presupuesto}}" placeholder="" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_fecha_inicio" class="col-sm-4 col-form-label col-form-label-sm">Fecha Inicio:</label>
                            <input type="date" id="tablas_orden_trabajo_fecha_inicio" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Fecha_Inicio}}" placeholder="" >
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_fecha_final" class="col-sm-4 col-form-label col-form-label-sm">Fecha Final:</label>
                            <input type="date" id="tablas_orden_trabajo_fecha_final" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Fecha_Final}}" placeholder="" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_observacion1" class="col-sm-2 col-form-label col-form-label-sm">Obsevacion 01:</label>
                            <textarea class="col-sm-10 form-control form-control-sm" id="tablas_orden_trabajo_observacion1" rows="3" placeholder="Obsevacion 1">{{datos.Observacion}}</textarea>
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_observacion2" class="col-sm-2 col-form-label col-form-label-sm">Obsevacion 02:</label>
                            <textarea class="col-sm-10 form-control form-control-sm" id="tablas_orden_trabajo_observacion2" rows="3" placeholder="Obsevacion 2">{{datos.Observacion2}}</textarea>
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_orden_trabajo_observacion3" class="col-sm-2 col-form-label col-form-label-sm">Obsevacion 03:</label>
                            <textarea class="col-sm-10 form-control form-control-sm" id="tablas_orden_trabajo_observacion3" rows="3" placeholder="Obsevacion 3">{{datos.Observacion3}}</textarea>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="d-none">
    {{#if datos}}
        <input type="text" id="tablas_orden_trabajo_fecha_inicio_predeterminado" value="S">
        <input type="text" id="tablas_orden_trabajo_fecha_final_predeterminado" value="S">
    {{else}}
        <input type="text" id="tablas_orden_trabajo_fecha_inicio_predeterminado" value="N">
        <input type="text" id="tablas_orden_trabajo_fecha_final_predeterminado" value="N">
    {{/if}}
</div>
<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_codigo" class="col-sm-4 col-form-label col-form-label-sm">Codigo:</label>
                            <input type="text" id="tablas_agencia_transporte_codigo" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Codigo}}" placeholder="Codigo" maxlength="50">
                        </div>
                    </form>
                </div>
                <div class="col-sm-2">

                </div>
                 <div class="col-sm-2">
                     <form class="m-0 p-0">
                        <div class="form-check row my-05 py-05">
                            <input class="form-check-input" type="checkbox" id="tablas_agencia_transporte_privado" >
                            <label class="form-check-label col-form-label-sm" for="tablas_agencia_transporte_privado">Privado</label>
                        </div>
                    </form>
                </div>
                <div class="col-sm-2">
                    <form class="m-0 p-0">
                        <div class="form-check row my-05 py-05">
                            <input class="form-check-input" type="checkbox" id="tablas_agencia_transporte_predeterminado" >
                            <label class="form-check-label col-form-label-sm" for="tablas_agencia_transporte_predeterminado">Predeterminado</label>
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_nombre" class="col-sm-2 col-form-label col-form-label-sm">Nombre:</label>
                            <input type="text" id="tablas_agencia_transporte_nombre" class="col-sm-10 form-control form-control-sm m-0 p-0"value="{{datos.Nombre}}" placeholder="Nombre" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_email" class="col-sm-4 col-form-label col-form-label-sm">Email:</label>
                            <input type="text" id="tablas_agencia_transporte_email" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Email}}" placeholder="E-mail" maxlength="50">
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_telefono1" class="col-sm-4 col-form-label col-form-label-sm">Telefono 1:</label>
                            <input type="text" id="tablas_agencia_transporte_telefono1" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Telefono1}}"  placeholder="Telefono 1">
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_telefono2" class="col-sm-4 col-form-label col-form-label-sm">Telefono 2:</label>
                            <input type="text" id="tablas_agencia_transporte_telefono2" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Telefono2}}" placeholder="Telefono 2" maxlength="50">
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_telefono3" class="col-sm-4 col-form-label col-form-label-sm">Telefono 3:</label>
                            <input type="text" id="tablas_agencia_transporte_telefono3" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Telefono3}}"placeholder="Telefono 3" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_web" class="col-sm-2 col-form-label col-form-label-sm">Web:</label>
                            <input type="text" id="tablas_agencia_transporte_web" class="col-sm-10 form-control form-control-sm m-0 p-0"value="{{datos.Web}}" placeholder="Web" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_direccion" class="col-sm-2 col-form-label col-form-label-sm">Direccion:</label>
                            <input type="text" id="tablas_agencia_transporte_direccion" class="col-sm-10 form-control form-control-sm m-0 p-0"value="{{datos.Direccion}}" placeholder="Direccion" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_ruc" class="col-sm-4 col-form-label col-form-label-sm">Ruc:</label>
                            <input type="text" id="tablas_agencia_transporte_ruc" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Ruc}}" placeholder="Ruc">
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_contacto" class="col-sm-4 col-form-label col-form-label-sm">Contacto:</label>
                            <input type="text" id="tablas_agencia_transporte_contacto" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Contacto}}"  placeholder="Contacto">
                        </div>
                    </form>
                </div>
            </div>
             <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_observacion1" class="col-sm-2 col-form-label col-form-label-sm">Observacion 1:</label>
                            <input type="text" id="tablas_agencia_transporte_observacion1" class="col-sm-10 form-control form-control-sm m-0 p-0"value="{{datos.Observacion1}}" placeholder="Observacion 1" >
                        </div>
                    </form>
                </div>
            </div>
             <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_agencia_transporte_observacion2" class="col-sm-2 col-form-label col-form-label-sm">Observacion 2:</label>
                            <input type="text" id="tablas_agencia_transporte_observacion2" class="col-sm-10 form-control form-control-sm m-0 p-0"value="{{datos.Observacion2}}" placeholder="Observacion 2" >
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="d-none">
    {{#if datos}}
        <input id="tablas_agencia_transporte_privado_predeterminado" type="text" value="{{datos.Privado}}">
        <input id="tablas_agencia_transporte_predeterminado_predeterminado" type="text" value="{{datos.Predeterminado}}">
    {{else}}
        <input id="tablas_agencia_transporte_privado_predeterminado" type="text" value="N">
        <input id="tablas_agencia_transporte_predeterminado_predeterminado" type="text" value="0">
    {{/if}}
</div>
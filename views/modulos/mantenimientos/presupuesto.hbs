<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_presupuesto_codigo" class="col-sm-4 col-form-label col-form-label-sm">Codigo:</label>
                            <input type="text" id="tablas_presupuesto_codigo" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Codigo}}" placeholder="Codigo" maxlength="50">
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_presupuesto_abreviatura" class="col-sm-4 col-form-label col-form-label-sm">Abreviatura:</label>
                            <input type="text" id="tablas_presupuesto_abreviatura" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Abreviatura}}" placeholder="Abreviatura" maxlength="50">
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_presupuesto_nombre" class="col-sm-2 col-form-label col-form-label-sm">Nombre:</label>
                            <input type="text" id="tablas_presupuesto_nombre" class="col-sm-10 form-control form-control-sm m-0 p-0"value="{{datos.Nombre}}" placeholder="Nombre" >
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_presupuesto_responsable" class="col-sm-4 col-form-label col-form-label-sm">Responsable:</label>
                            <input type="text" id="tablas_presupuesto_responsable" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Responsable}}" placeholder="Responsable" >
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_presupuesto_nivel" class="col-sm-4 col-form-label col-form-label-sm">Nivel:</label>
                            <input type="number" id="tablas_presupuesto_nivel" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Nivel}}" placeholder="Nivel">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="d-none">
    {{#if datos}}

    {{else}}

    {{/if}}
</div>
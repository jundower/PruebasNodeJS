<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_vehiculo_codigo" class="col-sm-4 col-form-label col-form-label-sm">Codigo:</label>
                            <input type="text" id="tablas_vehiculo_codigo" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Codigo}}" placeholder="Codigo" maxlength="50">
                        </div>
                    </form>
                </div>
                <div class="col-sm-2">

                </div>
                 <div class="col-sm-2">
                     <form class="m-0 p-0">
                        <div class="form-check row my-05 py-05">
                            <input class="form-check-input" type="checkbox" id="tablas_vehiculo_privado" >
                            <label class="form-check-label col-form-label-sm" for="tablas_vehiculo_privado">Privado</label>
                        </div>
                    </form>
                </div>
                <div class="col-sm-2">
                    <form class="m-0 p-0">
                        <div class="form-check row my-05 py-05">
                            <input class="form-check-input" type="checkbox" id="tablas_vehiculo_predeterminado" >
                            <label class="form-check-label col-form-label-sm" for="tablas_vehiculo_predeterminado">Predeterminado</label>
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_vehiculo_nombre" class="col-sm-4 col-form-label col-form-label-sm">Marca:</label>
                            <input type="text" id="tablas_vehiculo_nombre" class="col-sm-8 form-control form-control-sm m-0 p-0"value="{{datos.Nombre}}" placeholder="Marca" >
                        </div>
                    </form>
                </div>
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_vehiculo_placa" class="col-sm-4 col-form-label col-form-label-sm">Placa:</label>
                            <input type="text" id="tablas_vehiculo_placa" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.Placa}}" placeholder="Placa">
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <form class="m-0 p-0">
                        <div class="form-group row my-05 py-05">
                            <label for="tablas_vehiculo_numero_inscripcion" class="col-sm-4 col-form-label col-form-label-sm">N° Inscripcion:</label>
                            <input type="text" id="tablas_vehiculo_numero_inscripcion" class="col-sm-8 form-control form-control-sm m-0 p-0" value="{{datos.N_inscripcion}}" placeholder="N° Inscripcion">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="d-none">
    {{#if datos}}
        <input id="tablas_vehiculo_privado_predeterminado" type="text" value="{{datos.Privado}}">
        <input id="tablas_vehiculo_predeterminado_predeterminado" type="text" value="{{datos.Predeterminado}}">
    {{else}}
        <input id="tablas_vehiculo_privado_predeterminado" type="text" value="N">
        <input id="tablas_vehiculo_predeterminado_predeterminado" type="text" value="0">
    {{/if}}
</div>